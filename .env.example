# Environment Variables - DO NOT COMMIT THIS FILE
# Copy to .env and configure with your credentials

# MongoDB Configuration
MONGODB_URI=mongodb+srv://your-user:your-password@cluster0.huvfbuk.mongodb.net/MS_SEARCH_AND_BOOKING?retryWrites=true&w=majority&appName=Cluster0

# RabbitMQ Configuration
RABBITMQ_HOST=your-host.lmq.cloudamqp.com
RABBITMQ_PORT=5671
RABBITMQ_USERNAME=your-username
RABBITMQ_PASSWORD=your-password
RABBITMQ_VIRTUAL_HOST=your-virtual-host
RABBITMQ_SSL_ENABLED=true

# RabbitMQ Exchanges
RABBITMQ_EXCHANGE_BOOKING=booking.exchange

# RabbitMQ Queues
RABBITMQ_QUEUE_BOOKING_CREATED=booking.created.queue

# RabbitMQ Routing Keys
RABBITMQ_ROUTING_BOOKING_CREATED=booking.created
